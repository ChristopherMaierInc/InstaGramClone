<% @photos.each do |photo| %>

<div class="insta-photo">
  <%= image_tag photo.image_url, alt: photo.comment %><br>
  <h2><%= photo.comment %></h2>
  <% if photo.user %>
  <%= photo.user.email %><br>
  <% end %>
  <br>
  <%= link_to 'Show', photo %>
  <%= link_to 'Edit', edit_photo_path(photo) %>
  <%= link_to 'Destory', photo, method: :delete, date: {
    confirm: 'Are you sure?' } %>

</div>

<% end %>

<br>

<%= link_to 'New Photo', new_photo_path %>
